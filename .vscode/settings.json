{
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.exclude": {
        "**/*.build": true,
        "**/*.pyc": true,
        "**/__pycache__": true,
        "**/*.orig": true,
        "**/*.tmp": true,
        "output" : true,
    },
    "restructuredtext.confPath": "",
    "python.formatting.provider": "black",
    "python.analysis.extraPaths": ["Nuitka-factory"],
    "cSpell.words": [
        "appdirs",
        "backport",
        "backported",
        "bytecode",
        "ccache",
        "CEST",
        "clcache",
        "conda",
        "ctypes",
        "devel",
        "distutils",
        "Doxygen",
        "elif",
        "EPEL",
        "importlib",
        "Impressum",
        "isort",
        "libc",
        "libpython",
        "lxml",
        "metaclass",
        "MSVC",
        "nofollow",
        "Nuitka",
        "onefile",
        "Podman",
        "pyenv",
        "pyproject",
        "pyside",
        "pystone",
        "PYTHONHASHSEED",
        "PYTHONPATH",
        "redist",
        "Roadmap",
        "Scons",
        "Setuptools",
        "Tkinter",
        "traceback",
        "tracebacks",
        "ucrt",
        "uncompiled",
        "whitebox"
    ],
}
